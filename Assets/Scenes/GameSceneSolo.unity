          \u03BC\u03C0\u03BF\u03C5\u03BA\u03B1\u03BB\u03B9\u03CE\u03BD (\u03C0\u03BF\u03C5
          \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C0\u03B9\u03BF \u03C6\u03C4\u03B7\u03BD\u03AC
          \u03C3\u03C4\u03B7\u03BD \u03B1\u03B3\u03BF\u03C1\u03AC) \u03AE \u03B5\u03C0\u03B1\u03BD\u03B1\u03C7\u03C1\u03B7\u03C3\u03B9\u03BC\u03BF\u03C0\u03BF\u03B9\u03BF\u03CD\u03BC\u03B5\u03BD\u03C9\u03BD
          \u03C0\u03BF\u03C4\u03B7\u03C1\u03B9\u03CE\u03BD (\u03C0\u03BF\u03C5 \u03B5\u03AF\u03BD\u03B1\u03B9
          \u03C0\u03B9\u03BF \u03B4\u03B1\u03C0\u03B1\u03BD\u03B7\u03C1\u03AC \u03C3\u03C4\u03B7\u03BD
          \u03B1\u03B3\u03BF\u03C1\u03AC).\u03A0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1
          \u03BC\u03BF\u03B9\u03C1\u03B1\u03C3\u03C4\u03B5\u03AF\u03C4\u03B5 \u03C4\u03B9\u03C2
          \u03C3\u03BA\u03AD\u03C8\u03B5\u03B9\u03C2 \u03C3\u03B1\u03C2.\u03A4\u03B9
          \u03B8\u03B1 \u03BA\u03AC\u03BD\u03B1\u03C4\u03B5;"
        answers:
        - "\u03A0\u03C1\u03BF\u03C9\u03B8\u03AE\u03C3\u03C4\u03B5 \u03C4\u03B1 \u03C0\u03BB\u03B1\u03C3\u03C4\u03B9\u03BA\u03AC
          \u03BC\u03C0\u03BF\u03C5\u03BA\u03AC\u03BB\u03B9\u03B1 \u03B5\u03C0\u03B5\u03B9\u03B4\u03AE
          \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C6\u03B8\u03B7\u03BD\u03CC\u03C4\u03B5\u03C1\u03B1
          \u03BA\u03B1\u03B9 \u03C0\u03B5\u03C1\u03B9\u03C3\u03C3\u03CC\u03C4\u03B5\u03C1\u03BF\u03B9
          \u03BC\u03B1\u03B8\u03B7\u03C4\u03AD\u03C2 \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BD
          \u03BD\u03B1 \u03C4\u03B1 \u03B1\u03B3\u03BF\u03C1\u03AC\u03C3\u03BF\u03C5\u03BD."
        - "\u03A0\u03C1\u03BF\u03C9\u03B8\u03AE\u03C3\u03C4\u03B5 \u03C0\u03C1\u03BF\u03C3\u03C9\u03C1\u03B9\u03BD\u03AC
          \u03C4\u03B1 \u03C0\u03BB\u03B1\u03C3\u03C4\u03B9\u03BA\u03AC \u03BC\u03C0\u03BF\u03C5\u03BA\u03AC\u03BB\u03B9\u03B1
          \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B9\u03B1\u03C4\u03B7\u03C1\u03AE\u03C3\u03B5\u03C4\u03B5
